<%= simple_form_for(@school) do |f| %>
  <%= f.error_notification %>

  <%= f.input :name %>
  <%= f.input :about %>
  <%= f.input :address %>
  <%= f.input :postal_code %>
  <%= f.input :city %>
  <%= f.input :country %>
  <%= f.input :website %>
  <%= f.input :phone %>

  <%= f.button :submit %>

<% end %>
